{
  "name": "socket",
  "description": "socket",
  "version": "1.0.0",
  "author": {
    "name": "Phat Pham",
    "email": "phatpham9@gmail.com",
    "url": "https://onroads.xyz"
  },
  "main": "index.js",
  "engines": {
    "node": ">=9"
  },
  "scripts": {
    "start": "node index.js"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "http-status-codes": "^1.3.0",
    "socket.io": "^2.0.4"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.1.0",
    "micro-dev": "latest"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "airbnb",
    "rules": {
      "no-underscore-dangle": [
        2,
        {
          "allow": [
            "_id",
            "__v"
          ]
        }
      ],
      "no-param-reassign": [
        1,
        {
          "props": false
        }
      ],
      "func-names": [
        "error",
        "never"
      ],
      "no-await-in-loop": 0,
      "no-restricted-syntax": [
        "error",
        "WithStatement",
        "BinaryExpression[operator='in']"
      ],
      "linebreak-style": 0
    }
  }
}
